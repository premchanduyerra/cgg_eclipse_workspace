package Exceptions;

public class Exceptions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			int a=100/0;
		}
		catch(ArithmeticException e) {
			System.out.println("arthemetic exception");

		}
		catch(Exception e) {
			System.out.println(e);
		}

	}

}
