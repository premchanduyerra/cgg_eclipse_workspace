package Inheritance;

 class Animal1{
	void eat() {
		System.out.println("eating");
	}
}
class dog1 extends Animal1{
	void bark() {
		System.out.println("bark......");
	}
}

class cat1 extends dog1{
	void meow() {
		System.out.println("meow......");
	}
}

public class Heirarical {
public static void main(String args[]) {
	cat1 c=new cat1();
	c.eat();
	c.meow();
	
	dog d=new dog();
	d.bark();
}
}


