package array;

public class Arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int arr[]= {1,2,3,4,5,6};
		System.out.println("--> ");

				for (int i=0;i<arr.length;i++) {
					System.out.println("--> "+arr[i]);
				}
				
				System.out.println("<--");

	}

}
