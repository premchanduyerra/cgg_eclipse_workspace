package Agregation;

 class Employee{
	String name;
	int id;
	Address address;
	public Employee(int id,String name,Address address) {
		this.address=address;
		this.id=id;
		this.name=name;
				
	}
	
	void display() {
		System.out.println(id+" "+name);
		System.out.println(address.city+" "+address.state);

	}
}

 
 


